// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Articles {
  id          Int    @id @default(autoincrement())
  title       String @unique
  description String
  price       Int
  currency    String
  brand       String
}
